{% extends "_layouts/root" %}

{% block base %}

    {# Eager loading - Entry fields #}
    {% do craft.app.elements.eagerLoadElements(
      className(entry),
      [entry],
      [
      'featuredImage',
      'categories'
      ]
    ) %}

    {# Eager loading - Globals #}
    {% do craft.app.elements.eagerLoadElements(
      className(defaults),
      [defaults],
      [
      'logo',
      'fallbackImage'
      ]
    ) %}

    {% include "_partials/header.twig" %}

    <main>
        {% block content %}{% endblock %}

        {% if entry.slug|default() == "cookies" %}

            <section class="l-section l-section--has-no-padding-horizontal l-section--has-padding-vertical " style="background-color: #FFFFFF;">
                <div class="c-text-block ">
                    <div class="c-text-block__content">
                        <div class="t-wysiwyg">
                        
                            <script id="CookieDeclaration" src="https://consent.cookiebot.com/4d221050-29b6-41c5-aa6c-288df2be7470/cd.js" type="text/javascript" async></script>
                            
                        </div>
                    </div>
                </div>
            </section>            
        {% endif %}
    </main>

    {% include '_partials/partnerSlider.twig' %}
    {% include "_partials/footer.twig" %}
    
    {% include "_partials/modal.twig" %}

{% endblock %}